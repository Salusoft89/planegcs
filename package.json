{
	"name": "planegcs-wrapper",
	"version": "0.0.1",
	"private": true,
	"main": "index.ts",
	"scripts": {
		"build:em": "docker run --rm -v $(pwd)/planegcs:/src gcs_builder ./build_emscripten.sh",
		"build:bindings": "docker run --rm -v $(pwd)/planegcs:/src gcs_builder sh -c 'cd build_bindings && npm install && ./process_all.sh'",
		"build:docker": "docker build -t gcs_builder .",
		"build": "npm run build:docker && npm run build:em",
		"test": "jest"
	},
	"type": "module",
	"devDependencies": {
		"@babel/preset-env": "^7.20.2",
		"@types/jest": "^29.4.0",
		"babel-plugin-transform-import-meta": "^2.2.0",
		"jest": "^29.4.2",
		"ts-jest": "^29.0.5",
		"typescript": "^4.9.5"
	},
	"dependencies": {
		"@mathigon/euclid": "^1.1.8",
		"nunjucks": "^3.2.3",
		"tree-sitter": "^0.20.1",
		"tree-sitter-cpp": "^0.20.0"
	}
}
